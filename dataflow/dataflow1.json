{
	"name": "dataflow1",
	"properties": {
		"folder": {
			"name": "2"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DB_misumi_item",
						"type": "DatasetReference"
					},
					"name": "source1"
				},
				{
					"dataset": {
						"referenceName": "DB_misumi_prods",
						"type": "DatasetReference"
					},
					"name": "source2"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "join1"
				},
				{
					"name": "select2"
				},
				{
					"name": "select3"
				}
			],
			"scriptLines": [
				"source(output(",
				"          categoryCode as string,",
				"          categoryName as string,",
				"          seriesCode as string,",
				"          seriesName as string,",
				"          brandName as string,",
				"          {earliestShipping (Days)} as string,",
				"          description as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source1",
				"source(output(",
				"          {Item } as string,",
				"          {Sub Category} as string,",
				"          Category as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source2",
				"select2, source2 join(seriesName == {Sub Category},",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join1",
				"source1 select(mapColumn(",
				"          categoryCode,",
				"          categoryName,",
				"          seriesCode,",
				"          seriesName,",
				"          brandName,",
				"          description",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select2",
				"join1 select(mapColumn(",
				"          categoryCode,",
				"          categoryName,",
				"          seriesCode,",
				"          seriesName,",
				"          brandName,",
				"          description,",
				"          {Item },",
				"          {Sub Category},",
				"          Category",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select3"
			]
		}
	}
}